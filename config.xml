<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>pointWu Eat</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <icon src="www/img/logo.png" />
    <platform name="android">
        <icon src="www/img/logo.png" density="mdpi" />
        <icon src="www/img/logo.png" density="hdpi" />
        <icon src="www/img/logo.png" density="xhdpi" />
        <icon src="www/img/logo.png" density="xxhdpi" />
        <icon src="www/img/logo.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <icon src="www/img/logo.png" width="60" height="60" />
        <icon src="www/img/logo.png" width="76" height="76" />
        <icon src="www/img/logo.png" width="120" height="120" />
        <icon src="www/img/logo.png" width="152" height="152" />
        <icon src="www/img/logo.png" width="180" height="180" />
    </platform>
</widget>
